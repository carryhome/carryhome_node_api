{
  "name": "carryhome_node_api",
  "version": "1.0.0",
  "description": "CarryHome API",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "lint": "npx eslint \"**/*.js\"",
    "dev": "set NODE_ENV=multi-tenant&&nodemon src/app.ts",
    "multi-tenant": "set NODE_ENV=multi-tenant&&nodemon dist/app.js --exec babel-node -e js",
    "build": "tsc -p ."
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  }
}
